.userWrapper
  .mypageContent
    .userTitle
      = @user.nickname
      のマイページ
    .userTitle__image
    .userTitle__information
      %p
        = @user.nickname
      %p
        = Gender.find(@user.gender_id).name
      %p
        = Prefecture.find(@user.residence_id).name
  .contents
    - @posts.each do |post|
      = render partial: "posts/post", locals: { post: post }
    = paginate(@posts)
